<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Leaftaps automation" verbose="5">
	<parameter name="url" value="http://leaftaps.com/opentaps/" />
	<parameter name="browser" value="chrome" />
	<test thread-count="5" name="Create-Lead-Test">
		<classes>
			<class name="com.leaftaps.ui.leads.testcases.CreateLead" />
			<class name="com.leaftaps.ui.leads.testcases.EditLead" />
			<class name="com.leaftaps.ui.leads.testcases.DeleteLead" />
			<class name="com.leaftaps.ui.leads.testcases.DuplicateLead" />
			<class name="com.leaftaps.ui.leads.testcases.MergeLead" />
		</classes>
	</test> <!-- Create-Lead-Test -->
</suite> <!-- Leaftaps automation -->
